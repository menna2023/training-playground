pipeline {
    agent { 
		docker { 
			image 'alpine:latest' 
		}
	}
    stages {
        stage('build') {
            steps {
                script {
                	def buildConfigs = readJSON text: '{ name : "menna"}'
                	buildConfigs.each{ buildConfig ->
                		sh "echo '$buildConfig.name' "
					}
                }
            }
        }
    }
}
